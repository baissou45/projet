function controle(origin,pos) {


var typedebiens = document.getElementById('basic').value;

var nombredelot = parseInt(document.getElementById('inputnbrlot').value);

if (typedebiens=="Parcelle")
{
    document.getElementById('nbrlot').style.display = 'none';
    document.getElementById('caracteristique_parcelle').style.display = 'block';
    document.getElementById('habitat').style.display = 'none';
}
else if (typedebiens=="Batis") 
{


    document.getElementById('nbrlot').style.display = 'block';
    document.getElementById('caracteristique_parcelle').style.display = 'none';
    document.getElementById('habitat').style.display = 'block';
   
    if (origin!='typelot') {
    for (var i = 1; i < nombredelot+1; i++) {
        
         duplic('lot',i);
    }
    }


    if (document.getElementById('typedelot'+ pos).value =="Habitation") {
        document.getElementById('habitation'+ pos).style.display = 'block';
        document.getElementById('bureau'+ pos).style.display = 'none';
        document.getElementById('boutique'+ pos).style.display = 'none';
        document.getElementById('lotimage'+ pos).style.display = 'block';
    }
    else if (document.getElementById('typedelot'+ pos).value=="Bureau") {
        document.getElementById('habitation'+ pos).style.display = 'none';
        document.getElementById('bureau'+ pos).style.display = 'block';
        document.getElementById('boutique'+ pos).style.display = 'none';
        document.getElementById('lotimage'+ pos).style.display = 'block';
    }
    else if (document.getElementById('typedelot'+ pos).value=="Magasin ou Boutique") {
        document.getElementById('habitation'+ pos).style.display = 'none';
        document.getElementById('bureau'+ pos).style.display = 'none';
        document.getElementById('boutique'+ pos).style.display = 'block';
        document.getElementById('lotimage'+ pos).style.display = 'block';
    }
    else{
        document.getElementById('habitation'+ pos).style.display = 'none';
        document.getElementById('bureau'+ pos).style.display = 'none';
        document.getElementById('boutique'+ pos).style.display = 'none';
        document.getElementById('lotimage'+ pos).style.display = 'none';
    }
}

else{
    document.getElementById('nbrlot').style.display = 'none';
    document.getElementById('caracteristique_parcelle').style.display = 'none';
    document.getElementById('habitat').style.display = 'none';
}

}
